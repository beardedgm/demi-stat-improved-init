{
  "manifest_version": 3,
  "name": "Demiplane Stat Extractor",
  "version": "1.0",
  "description": "Extracts character/creature stats from a Demiplane page and converts them to JSON.",
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}